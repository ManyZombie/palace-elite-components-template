{
  "name": "pe-components",
  "version": "0.2.5",
  "files": [
    "dist"
  ],
  "main": "./dist/pe-components.umd.js",
  "module": "./dist/pe-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/pe-components.es.js",
      "require": "./dist/pe-components.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ManyZombie/pe-components-template.git"
  },
  "homepage": "https://github.com/ManyZombie/pe-components-template.git#readme",
  "types": "./dist/src/index.d.ts",
  "keywords": [
    "vue2",
    "template"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write"
  },
  "dependencies": {
    "vue": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue": "^6.5.9",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/runtime-dom": "^3.2.37",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.5.13",
    "eslint-plugin-vue": "^9.1.1",
    "postcss": "^8.4.14",
    "sass": "^1.52.3",
    "storybook-builder-vite-vue2": "^0.1.32",
    "tailwindcss": "^3.1.4",
    "typescript": "~4.3.5",
    "vite": "^2.6.3",
    "vite-plugin-eslint": "^1.6.1",
    "vite-plugin-vue2": "^2.0.1",
    "vue-loader": "^15.10.0",
    "vue-template-compiler": "^2.7.0",
    "vue-tsc": "^0.29.5"
  }
}
